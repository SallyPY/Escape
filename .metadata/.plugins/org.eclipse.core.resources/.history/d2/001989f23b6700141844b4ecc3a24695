package escape;

import javax.swing.*;
import java.awt.*;
import java.awt.event.*;
import javax.swing.border.*;


public class EscapeGame extends JFrame{

	private EscapeGame() {
		super("Escape!");
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		setContentPane(new MainScreen());
		pack();
		setVisible(true);
		}
	
	public static void main (String[] args) {
	 new EscapeGame();
	}


}
